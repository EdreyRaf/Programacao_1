#include <stdio.h>
char a;

int main()
{
    printf("Digite uma letra: ");
    scanf("\n%s", &a);
    
    switch(a){
        case('A'):
            printf("Exelente");
            break;
        case('B'):
            printf("Ã“timo");
            break;
        case('C'):
            printf("Bom");
            break;
        case('D'):
            printf("Regular");
            break;
        case('E'):
            printf("Ruim");
            break;
        case('F'):
            printf("Nos vemos ano que vem!");
            break;
        default:
            printf("Conceito Inexistente");
    }   
}
