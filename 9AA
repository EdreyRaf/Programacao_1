#include <stdio.h>
int a,b;
int main()
{
    printf("Digite um dia ");
    scanf("\n%d", &a);
    printf("Digite um mes ");
    scanf("\n%d", &b);
    if(b>=1&&b<=12){
        if(b==1||b==3||b==5||b==7||b==8||b==10||b==12){
            if(a>=1&&a<=31){
                printf("a data %d/%d existe",a,b);
            }else{
                printf("a data %d/%d nao existe",a,b);
            } }else if(b==2){
                if(a>=1&&a<=28){
                printf("a data %d/%d existe",a,b);
            }else{
                printf("a data %d/%d nao existe",a,b);
            }
        } else {
            if(a>=1&&a<=30){
                printf("a data %d/%d existe",a,b);
            }else{
                printf("a data %d/%d nao existe",a,b);
        }
    }
        return 0;
}}
